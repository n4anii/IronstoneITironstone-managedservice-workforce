{
	"name": "GlobalLevelTemplate",
	"visualStyles": {
	
		"basicShape": {
			"*": {
				"line": [{
					"lineColor": { "solid": { "color": "#ffdd40"}},
					"transparency": 0,
					"weight": 3,
					"roundEdge": 0,
					"fontFamily": "Verdana"
				}],
				"fill": [{
					"show": true,
					"fillColor": { "solid": { "color": "#ffdd40"}},
					"transparency": 0
				}],
				"rotation": [{
					"angle": 0
				}],
				"visualHeader": [{
					"show": false
				}]
			}
		},
		"textbox": {
			"*": {
				"background": [{
					"show": false,
					"color": { "solid": { "color": "#DDDDDD"}},
					"transparency": 0
				}],
				"visualHeader": [{
					"show": true
				}]
			}
		},
		"pieChart": {
			"*": {
				"legend": [{
					"show": true,
					"position": "Top",
					"showTitle": false,
					"titleText": "",
					"legendColor": { "solid": { "color": "#3f4b56"}},
					"fontFamily": "Segoe UI",
					"fontSize": 12
				}],
				"labels": [{
					"show": true
				}]
			}
		},
		"tableEx": {
			"*": {
				"grid": [{
					"gridVertical": false,
					"gridVerticalColor": { "solid": { "color": "#ffdd40"}},
					"gridVerticalWeight": 2,
					"gridHorizontal": false,
					"gridHorizontalColor": { "solid": { "color": "#ffdd40"}},
					"gridHorizontalWeight": 2,
					"rowPadding": 2,
					"outlineColor": { "solid": { "color": "#ffdd40"}},
					"outlineWeight": 2,
					"textSize": 12,
					"imageHeight": 100
				}],
				"columnHeaders": [{
					"fontColor": { "solid": { "color": "#3f4b56"}},
					"backColor": { "solid": { "color": "#ffffff"}},
					"outline": "BottomOnly",
					"autoSizeColumnWidth": true,
					"fontFamily": "Verdana",
					"fontSize": 12,
					"alignment": "Center",
					"wordWrap": true
				}],
				"values": [{
					"fontColorPrimary": { "solid": { "color": "#3f4b56"}},
					"backColorPrimary": { "solid": { "color": "#ffffff"}},
					"fontColorSecondary": { "solid": { "color": "#333333"}},
					"backColorSecondary": { "solid": { "color": "#ffffff"}},
					"outline": "None",
					"urlIcon": true,
					"wordWrap": true,
					"fontFamily": "Verdana",
					"fontSize": 8
				}],
				"total": [{
					"totals": true,
					"fontColor": { "solid": { "color": "#3f4b56"}},
					"backColor": { "solid": { "color": "#ffffff"}},
					"outline": "TopOnly",
					"fontFamily": "Verdana",
					"fontSize": 10
				}]
			}
		},
		"card": {
			"*": {
				"labels": [{
					"color": { "solid": { "color": "#3f4b56"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 0,
					"fontSize": 16,
					"fontFamily": "Verdana"
				}],
				"categoryLabels": [{
					"show": true,
					"color": { "solid": { "color": "#3f4b56"}},
					"fontSize": 10,
					"fontFamily": "Verdana"
				}],
				"wordWrap": [{
					"show": true
				}],
				"background": [{
						"show": false,
						"color": {
							"solid": {
								"color": "#000000"
							}
						},
						"transparency": 0
					}
				],
				  "title": [{
				    "show": true,
				    "fontColor": { "solid": { "color": "#3f4b56" } },
				    "background": { "solid": { "color": "#ffffff" } },
				    "alignment": "left",
				    "fontSize": 11,
				    "fontFamily": "Verdana"
				  }],
				  "border": [{
				    "show": false,
				    "color": { "solid": { "color": "#062b60"}}
				  }]
			}
		},
        "*": {
            "*": {
                "title": [{
                    "show": true,
                    "fontColor": { "solid": { "color": "#3f4b56" } },
                    "background": { "solid": { "color": "#ffff" } },
                    "alignment": "center",
                    "fontSize": 12,
                    "fontFamily": "Verdana"
                }],
                "background": [{
                    "show": false
                }],
                "lockAspect": [{
                    "show": true
                }],
                "border": [{
                    "show": false
                }],
                "visualTooltip": [{
                    "type": "Default"
                }],
                "stylePreset": [{
                    "name": "None"
                }]
            }
        },
        "page": {
            "*": {
                "background": [{
                    "color": { "solid": { "color": "#465d85" } },
                    "transparency": 100
                }],
                "outspace": [{
                    "color": { "solid": { "color": "#886b01" } },
                    "transparency": 100
                }]
            }
        }
    }
}